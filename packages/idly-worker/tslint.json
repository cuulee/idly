{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-immutable"
  ],
  "jsRules": {},
  "rules": {
    "no-reference": false,
    "interface-name": [
      false
    ],
    "quotemark": [
      "single"
    ],
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-console": [
      false
    ],
    "arrow-parens": false,
    "curly": false,
    "no-unused-variable": [
      false,
      {
        "ignore-pattern": "^_"
      }
    ],
    "only-arrow-functions": [
      false
    ],
    "no-var-requires": false,
    "semicolon": [
      false
    ],
    "no-unused-new": true,
    "no-unused-expression": [
      true
    ],
    "no-unused-imports": true,
    "max-line-length": [
      false
    ],
    "cyclomatic-complexity": [
      false
    ],
    "member-access": [
      false
    ],
    "export-name": false,
    "no-constant-condition": false,
    "typedef": [
      false
    ],
    "no-reserved-keywords": false,
    "no-false": true,
    "object-literal-key-quotes": [
      false
    ]
  },
  "rulesDirectory": []
}