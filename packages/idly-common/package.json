  "devDependencies": {
    "@types/jest": "^20.0.8",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.2",
    "tslint": "^5.6.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
